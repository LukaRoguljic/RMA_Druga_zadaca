<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luka.convertme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ToDistance" />
        <activity android:name=".ToTemperature" />
        <activity android:name=".ToSpeed" />
        <activity android:name=".ResultTemperature" />
        <activity android:name=".ResultDistance" />
        <activity android:name=".ResultSpeed" />
        <activity android:name=".ToWeight" />
        <activity android:name=".ResultWeight"></activity>
    </application>

</manifest>